{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach Debug (CMSIS-DAP)",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/app/target/thumbv6m-none-eabi/release/midi-embedded",
            "request": "attach",
            "type": "cortex-debug",
            "targetId": "rp2040",
            "servertype": "pyocd",
            "device": "RP2040",
            "svdFile": "${workspaceRoot}/.vscode/rp2040.svd",
            "gdbPath": "/usr/bin/gdb",
            "gdbTarget": "localhost:5000",
            "showDevDebugOutput": "raw"
        }
    ]
}
